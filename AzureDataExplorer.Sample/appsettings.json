{
  "AzureDataExplorer": {
    "DatabaseName": "<DB_NAME>",
    "KustoUri": "https://<CLUSTER_NAME>.<REGION>.kusto.windows.net/",
    "IngestUri": "https://ingest-<CLUSTER_NAME>.<REGION>.kusto.windows.net/",
    "AppClientId": "<CLIENT_ID>",
    "AppClintSecret": "<CLIENT_SECRET>",
    "AppTenantId": "<TENANT_ID>"
  },

  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
